{
	"rules": {
		"no-string-throw": true,
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"class-name": true,
		"indent": [
			true,
			"tabs"
		],
		"linebreak-style": [
			true,
			"CRLF"
		],
		"member-access": true,
		"no-console": true,
		"ordered-imports": true,
		"quotemark": [
			true,
			"single"
		],
		"semicolon": [
			true,
			"always"
		],
		"triple-equals": true
	},
	"defaultSeverity": "warning"
}